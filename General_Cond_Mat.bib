@Article{Fradkin2010,
  author    = {Eduardo Fradkin and Steven A. Kivelson and Michael J. Lawler and James P. Eisenstein and Andrew P. Mackenzie},
  title     = {Nematic Fermi Fluids in Condensed Matter Physics},
  journal   = {Annual Review of Condensed Matter Physics},
