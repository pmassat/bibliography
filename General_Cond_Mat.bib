@Article{Slanic1998,
  author    = {Slani\v{c}, Z and D.P Belanger},
  title     = {The random-field specific heat critical behavior at high magnetic concentration: Fe0.93Zn0.07F2},
  journal   = {Journal of Magnetism and Magnetic Materials},
  year      = {1998},
  volume    = {186},
