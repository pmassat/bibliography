  file      = {:Chemistry\\Phase_equilibrium_diagrams\\Warzee1965.pdf:PDF},
  keywords  = {TmAsO4, TmVO4, flux growth, PbO-La2O3, Sm2O3, Gd2O3},
  publisher = {Blackwell Publishing Ltd},
  timestamp = {2017-10-17},
