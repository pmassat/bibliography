@InCollection{Morin1990,
  author    = {P. Morin and D. Schmitt},
  title     = {Chapter 1 Quadrupolar interactions and magneto-elastic effects in rare earth intermetallic compounds},
  booktitle = {Handbook of Ferromagnetic Materials},
  publisher = {Elsevier},
  year      = {1990},
  volume    = {5},
  series    = {Handbook of Ferromagnetic Materials},
  pages     = {1 - 132},
  abstract  = {Publisher Summary
The study of multipolar interactions concerning the unfilled 4f shell has known a great development in rare earth (R) intermetallic compounds. It involves the complex problems of both the crystalline electric field (CEF) acting on a 4f ion and the pair interactions between different ions. The electron–lattice coupling can drive very characteristic magneto–elastic features—in particular, in elastic constants and in magnetostriction, through symmetry lowering modes depending on the 4f electronic states. The magneto–elastic modulation of the CEF corresponds to a gain of energy larger than the elastic energy lost at the thermodynamic equilibrium. The quantum Hamiltonian describes both the one- and two-ion, spin, and quadrupolar couplings in systems of cubic, tetragonal, and hexagonal symmetries. The power of the susceptibility formalism, which allows independent determinations of the various coefficients, is discussed in this chapter. The main experimental evidence of quadrupolar orderings is presented along with the quadrupolar consequences on the magnetic properties. The experimental techniques which are based on the susceptibility formalism are illustrated.},
  doi       = {https://doi.org/10.1016/S1574-9304(05)80061-6},
  file      = {Morin1990.pdf:Articles_rare-earth_nematics\\Nematic_metals\\Morin1990.pdf:PDF},
  issn      = {1574-9304},
  timestamp = {2019-10-01},
  url       = {http://www.sciencedirect.com/science/article/pii/S1574930405800616},
}

