@Article{Hess1990,
  author    = {G Hess and M Dammann and H G Kahle and A Kasten and C Seifert and K Vogtlin},
  title     = {Investigations on the mixed Jahn-Teller system (Tb x , Dy 1-x )VO 4 . I. Phase diagram, specific heat and spectroscopic measurements},
  journal   = {Journal of Physics: Condensed Matter},
  year      = {1990},
  volume    = {2},
  number    = {5},
  pages     = {1073},
  abstract  = {At low temperatures TbVO 4 and DyVO 4 exhibit a structural phase transition from the same high-temperature structure to different low-temperature structures. Owing to symmetry different crystallographic domains are possible. Investigations of single crystals of the mixed system (Tb x ,Dy 1-x )VO 4 with a polarising microscope gave information about domain distribution and distortion type. Specific heat measurements, spectroscopic and birefringence data yielded the transition temperatures. The phase diagram of the mixed system may be interpreted as a superposition of the phase diagram of diluted TbVO 4 and that of diluted DyVO 4 . At high concentrations of one of the rare-earth ions the crystals distort like the respective pure substances, whereas at medium concentrations x approximately=0.4 a superposition of the distortion types is observed. The measurements are interpreted in the frame of a mean-field approximation, which is modified by the phenomenological compressible Ising model and by the inclusion of local strains of random distribution.},
  file      = {Hess1990.pdf:Articles_CJTE\\RFIM\\DyTbVO4_RFIM\\Hess1990.pdf:PDF},
  keywords  = {RVO4, RFIM, heat capacity, optical absorption, theory, compressible Ising model},
  timestamp = {2018-04-23},
  url       = {http://stacks.iop.org/0953-8984/2/i=5/a=003},
}

