@Article{Harley1971,
  author =    {R.T. Harley and W. Hayes and S.R.P. Smith},
  title =     {Raman study of phase transitions in rare earth vanadates},
  journal =   {Solid State Communications},
  year =      {1971},
  volume =    {9},
  number =    {9},
  pages =     {515 - 517},
  abstract =  {The phase transitions in DyVO4 at about 14K and in TbVO4 at about 33K have been investigated by Raman scattering. The Eg phonons are split below the transition temperature but the singlet phonon modes are not appreciably affected. No soft phonon mode is observed but large changes are seen in the low lying electronic levels. These results appear to be a consequence of a cooperative Jahn-Teller effect.Durch Messung der Raman-Streustrahlung werden die Phasenübergange von DyVO4 (14°K) und TbVO4 (33°K) untersucht. Unterhalb der Übergangstemperaturen werden Aufspaltungen der Eg-Phononen festgestellt, während die Singlett-Phononenzustände nur wenig beeinflusst sind. Eine “soft mode” im Phononenspektrum wird nicht beobachtet; die unteren elektronischen Zustände dagegen zeigen starke energetische Verschiebungen. Diese Beobachtungen lassen sich als Wirkungen eines Kollektiven Jahn-Teller-Effektes deuten.},
  doi =       {http://dx.doi.org/10.1016/0038-1098(71)90136-0},
  file =      {Harley1971.pdf:Articles_CJTE\\RVO4\\RVO4_Raman\\Harley1971.pdf:PDF},
  issn =      {0038-1098},
  keywords =  {TbVO4, DyVO4, nematicity, Raman},
  owner =     {Pierre},
  timestamp = {2017-06-27},
  url =       {http://www.sciencedirect.com/science/article/pii/0038109871901360}
}

