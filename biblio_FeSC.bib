@Article{Cho2016,
  author    = {Cho, Kyuil and Ko{\'n}czykowski, Marcin and Teknowijoyo, Serafim and Tanatar, Makariy A. and Liu, Yong and Lograsso, Thomas A. and Straszheim, Warren E. and Mishra, Vivek and Maiti, Saurabh and Hirschfeld, Peter J. and Prozorov, Ruslan},
  title     = {Energy gap evolution across the superconductivity dome in single crystals of (Ba1-xKx)Fe2As2},
  journal   = {Science Advances},
  year      = {2016},
  volume    = {2},
  number    = {9},
  abstract  = {The mechanism of unconventional superconductivity in iron-based superconductors (IBSs) is one of the most intriguing questions in current materials research. Among non-oxide IBSs, (Ba1-xKx)Fe2As2 has been intensively studied because of its high superconducting transition temperature and fascinating evolution of the superconducting gap structure from being fully isotropic at optimal doping (x ≈ 0.4) to becoming nodal at x \&gt; 0.8. Although this marked evolution was identified in several independent experiments, there are no details of the gap evolution to date because of the lack of high-quality single crystals covering the entire K-doping range of the superconducting dome. We conducted a systematic study of the London penetration depth, λ(T), across the full phase diagram for different concentrations of point-like defects introduced by 2.5-MeV electron irradiation. Fitting the low-temperature variation with the power law, Δλ ~ Tn, we find that the exponent n is the highest and the Tc suppression rate with disorder is the smallest at optimal doping, and they evolve with doping being away from optimal, which is consistent with increasing gap anisotropy, including an abrupt change around x ≃ 0.8, indicating the onset of nodal behavior. Our analysis using a self-consistent t-matrix approach suggests the ubiquitous and robust nature of s{\textpm} pairing in IBSs and argues against a previously suggested transition to a d-wave state near x = 1 in this system.},
  doi       = {10.1126/sciadv.1600807},
  eprint    = {http://advances.sciencemag.org/content/2/9/e1600807.full.pdf},
  file      = {Cho2016.pdf:Articles_FeSC\\122\\KBa122\\KBa122_London_penetration\\Cho2016.pdf:PDF},
  keywords  = {KBa122, London penetration depth, superconductivity, gap symmetry, doping evolution},
  publisher = {American Association for the Advancement of Science},
  timestamp = {2017-09-28},
  url       = {http://advances.sciencemag.org/content/2/9/e1600807},
}

