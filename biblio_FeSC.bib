@Article{Lu2014,
  Title                    = {Nematic spin correlations in the tetragonal state of uniaxial-strained BaFe2−xNixAs2},
  Author                   = {Lu, Xingye and Park, J. T. and Zhang, Rui and Luo, Huiqian and Nevidomskyy, Andriy H. and Si, Qimiao and Dai, Pengcheng},
  Journal                  = {Science},
  Year                     = {2014},
  Number                   = {6197},
  Pages                    = {657-660},
  Volume                   = {345},

  Abstract                 = {Understanding the microscopic origins of electronic phases in high-transition temperature (high-Tc) superconductors is important for elucidating the mechanism of superconductivity. In the paramagnetic tetragonal phase of BaFe2−xTxAs2 (where T is Co or Ni) iron pnictides, an in-plane resistivity anisotropy has been observed. Here, we use inelastic neutron scattering to show that low-energy spin excitations in these materials change from fourfold symmetric to twofold symmetric at temperatures corresponding to the onset of the in-plane resistivity anisotropy. Because resistivity and spin excitation anisotropies both vanish near optimal superconductivity, we conclude that they are likely intimately connected.},
  Doi                      = {10.1126/science.1251853},
  Eprint                   = {http://www.sciencemag.org/content/345/6197/657.full.pdf},
