  File                     = {Blomberg2013.pdf:Articles_FeSC/Ba122/KBa122/KBa122_resistivity/Blomberg2013.pdf:PDF},
  Keywords                 = {barium compounds, doping, Fermi surface, high-temperature superconductors, iron compounds, magnetic anisotropy, magnetoresistance, paramagnetism, pnictide superconductors, spin fluctuations, surface reconstruction},
  Owner                    = {pierre},
