@Article{Ingle2016,
  Title                    = {Evolution of structure, magnetic and transport properties of IMG [http://ej.iop.org/images/2053-1591/3/12/126001/toc_mrxaa4c73ieqn1.gif] {${\mathrm{Fe}}_{\mathrm{1-x}}{\mathrm{Mn}}_{x}\mathrm{Se}$}},
  Author                   = {Kapil E Ingle and K R Priolkar and P A Bhobe and A K Nigam},
  Journal                  = {Materials Research Express},
  Year                     = {2016},
  Number                   = {12},
  Pages                    = {126001},
  Volume                   = {3},

  Abstract                 = {The present paper seeks to investigate effect of Mn doping in superconducting FeSe. It is found that over the entire doping range in ##IMG## [http://ej.iop.org/images/2053-1591/3/12/126001/mrxaa4c73ieqn3.gif] {${\mathrm{Fe}}_{1-x}$} Mn x Se ( ##IMG## [http://ej.iop.org/images/2053-1591/3/12/126001/mrxaa4c73ieqn4.gif] {$0\leqslant x\leqslant 1$} ), Mn does not substitute Fe in the superconducting tetragonal phase. Instead two impurity phases, NiAs type hexagonal phase and NaCl type cubic phase, grow with increasing Mn content. Initially, hexagonal phase has a higher content than the cubic phase but beyond x = 0.5, the cubic phase grows rapidly and for ##IMG## [http://ej.iop.org/images/2053-1591/3/12/126001/mrxaa4c73ieqn5.gif] {$x\geqslant 0.8$} , the sample is monophasic with cubic NaCl type structure. The superconducting tetragonal phase content steadily decreases with increasing Mn concentration and completely disappears beyond x = 0.5. The premise that Mn never replaces Fe in the superconducting phase is further strengthened by observation of a sharp drop in AC susceptibility akin to superconducting transition at the T c of FeSe up to x  = 0.5. EXAFS studies at the Fe K edge also show that the Fe has a four coordinated tetragonal local structure in all compositions below x = 0.5, similar to that in FeSe and it gradually changes to a six coordinated one as is expected for a NaCl type cubic phase for ##IMG## [http://ej.iop.org/images/2053-1591/3/12/126001/mrxaa4c73ieqn6.gif] {$x\geqslant 0.5$} .},
  File                     = {Ingle2016.pdf:Articles_FeSC/FeSe/FeMnSe/Ingle2016.pdf:PDF},
  Keywords                 = {FeSeMn, structure, doping, magnetism, superconductivity, ac susceptibility, resistivity, EXAFS
not printed},
  Owner                    = {pierre},
  Timestamp                = {2017.01.31},
  Url                      = {http://stacks.iop.org/2053-1591/3/i=12/a=126001}
}

