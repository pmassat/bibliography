@Article{Lengyel2016,
  author    = {Lengyel, E. and Kumar, M. and Schnelle, W. and Leithe-Jasper, A. and Nicklas, M.},
  journal   = {physica status solidi (b)},
  title     = {Effect of Co doping and hydrostatic pressure on SrFe2 As2},
  year      = {2016},
  issn      = {1521-3951},
  pages     = {n/a--n/a},
  abstract  = {We report a pressure study on electron doped SrFe2−xCoxAs2 by electrical-resistivity (ρ) and magnetic-susceptibility (χ) experiments. Application of either external pressure or Co substitution rapidly suppresses the spin-density wave ordering of the Fe moments and induces superconductivity in SrFe2As2. At x=0.2 the broad superconducting (SC) dome in the T−−p phase diagram exhibits its maximum Tc,max=20K at a pressure of only pmax≈0.75GPa. In SrFe1.5Co0.5As2 no superconductivity is observed anymore up to 2.8 GPa. Upon increasing the Co concentration the maximum of the SC dome shifts toward lower pressure accompanied by a decrease in the value of Tc,max. Even though, superconductivity is induced by both tuning methods, Co substitution leads to a much more robust SC state. Our study evidences that in SrFe2−xCoxAs2 both, the effect of pressure and Co substitution, have to be considered in order to understand the SC phase-diagram and further attests the close relationship of SrFe2As2 and its sister compound BaFe2As2.},
  doi       = {10.1002/pssb.201600154},
  file      = {:Articles_FeSC/122/Sr122/LengyelE2016.pdf:PDF},
  keywords  = {iron-based superconductors, Pressure experiments, superconductivity, Sr122, doping, electrical resistivity, magnetic susceptibility, magnetism, superconductivity not printed},
  owner     = {pierre},
  timestamp = {2016.11.28},
  url       = {http://dx.doi.org/10.1002/pssb.201600154},
}

