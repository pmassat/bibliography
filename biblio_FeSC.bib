@Article{Si2009,
  Title                    = {Correlation effects in the iron pnictides},
  Author                   = {Qimiao Si and Elihu Abrahams and Jianhui Dai and Jian-Xin Zhu},
  Journal                  = {New Journal of Physics},
  Year                     = {2009},
  Number                   = {4},
  Pages                    = {045001},
  Volume                   = {11},

  Abstract                 = {One of the central questions about the iron pnictides concerns the extent to which their electrons are strongly correlated. Here, we address this issue through the phenomenology of the charge transport and dynamics, the single-electron excitation spectrum, and magnetic ordering and dynamics. We outline the evidence that the parent compounds, while metallic, have electron interactions that are sufficiently strong to produce incipient Mott physics. In other words, in terms of the strength of electron correlations compared with the kinetic energy, the iron pnictides are closer to intermediately coupled systems lying at the boundary between itinerancy and localization, such as V 2 O 3 or Se-doped NiS 2 , rather than to simple antiferromagnetic metals like Cr. This level of electronic correlations produces a new small parameter for controlled theoretical analysis, namely the fraction of the single-electron spectral weight that lies in the coherent part of the excitation spectrum. Using this expansion parameter, we construct the effective low-energy Hamiltonian and discuss its implications for the magnetic order and magnetic quantum criticality. Finally, this approach sharpens the notion of magnetic frustration for such a metallic system, and brings about a multiband matrix t – J 1 – J 2 model for the carrier-doped iron pnictides.},
  File                     = {:Articles_FeSC/Theory/Theory_FeSCs/SiQ2009.pdf:PDF},
  Keywords                 = {FeSC, theory, electronic correlations},
  Owner                    = {pierre},
  Timestamp                = {2016.11.04},
  Url                      = {http://stacks.iop.org/1367-2630/11/i=4/a=045001}
}

