@Article{Gresty2009,
  author    = {Gresty, Nathalie C. and Takabayashi, Yasuhiro and Ganin, Alexey Y. and McDonald, Martin T. and Claridge, John B. and Giap, Duong and Mizuguchi, Yoshikazu and Takano, Yoshihiko and Kagayama, Tomoko and Ohishi, Yasuo and Takata, Masaki and Rosseinsky, Matthew J. and Margadonna, Serena and Prassides, Kosmas},
  journal   = {Journal of the American Chemical Society},
  title     = {Structural Phase Transitions and Superconductivity in Fe1+δSe0.57Te0.43 at Ambient and Elevated Pressures},
  year      = {2009},
  note      = {PMID: 19863098},
  number    = {46},
  pages     = {16944-16952},
  volume    = {131},
  abstract  = {The ternary iron chalcogenide, Fe1.03Se0.57Te0.43 is a member of the recently discovered family of Fe-based superconductors with an ambient pressure Tc of 13.9 K and a simple structure comprising layers of edge-sharing distorted Fe(Se/Te)4 tetrahedra separated by a van der Waals gap. Here we study the relationship between its structural and electronic responses to the application of pressure. Tc depends sensitively on applied pressure attaining a broad maximum of 23.3 K at ∼3 GPa. Further compression to 12 GPa leads to a metallic but nonsuperconducting ground state. High-resolution synchrotron X-ray diffraction shows that the superconducting phase is metrically orthorhombic at ambient pressure but pressurization to ∼3 GPa leads to a structural transformation to a more distorted structure with monoclinic symmetry. The exact coincidence of the crystal symmetry crossover pressure with that at which Tc is maximum reveals an intimate link between crystal and electronic structures of the iron chalcogenide superconductors.},
  doi       = {10.1021/ja907345x},
  eprint    = {https://doi.org/10.1021/ja907345x},
  file      = {:Articles_FeSC\\FeSe\\FeSeTe\\FeSeTe_XRays\\Gretsy2009.pdf:PDF},
