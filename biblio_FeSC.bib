@Article{Wang2016e,
  Title                    = {Magnetic ground state of FeSe},
  Author                   = {Wang, Qisi and Shen, Yao and Pan, Bingying and Zhang, Xiaowen and Ikeuchi, K. and Iida, K. and Christianson, A. D. and Walker, H. C. and Adroja, D. T. and Abdel-Hafiez, M. and Chen, Xiaojia and Chareev, D. A. and Vasiliev, A. N. and Zhao, Jun},
  Journal                  = {Nat Commun},
  Year                     = {2016},

  Month                    = jul,
  Pages                    = {--},
  Volume                   = {7},

  Abstract                 = {Elucidating the nature of the magnetism of a high-temperature superconductor is crucial for establishing its pairing mechanism. The parent compounds of the cuprate and iron-pnictide superconductors exhibit Neel and stripe magnetic order, respectively. However, FeSe, the structurally simplest iron-based superconductor, shows nematic order (Ts=90[thinsp]K), but not magnetic order in the parent phase, and its magnetic ground state is intensely debated. Here we report inelastic neutron-scattering experiments that reveal both stripe and Neel spin fluctuations over a wide energy range at 110[thinsp]K. On entering the nematic phase, a substantial amount of spectral weight is transferred from the Neel to the stripe spin fluctuations. Moreover, the total fluctuating magnetic moment of FeSe is [sim]60% larger than that in the iron pnictide BaFe2As2. Our results suggest that FeSe is a novel S=1 nematic quantum-disordered paramagnet interpolating between the Neel and stripe magnetic instabilities.},
  Comment                  = {Supplementary information available for this article at http://www.nature.com/ncomms/2016/160719/ncomms12182/suppinfo/ncomms12182_S1.html},
  File                     = {Wang2016e.pdf:Articles_FeSC/FeSe/FeSe_neutrons/Wang2016e.pdf:PDF},
  Keywords                 = {FeSe, neutrons scattering, magnetism, nematicity},
  Owner                    = {pierre},
  Publisher                = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  Timestamp                = {2016.07.30},
  Url                      = {http://dx.doi.org/10.1038/ncomms12182}
}

@Article{Wang2016f,
  Title                    = {Interface high-temperature superconductivity},
  Author                   = {Lili Wang and Xucun Ma and Qi-Kun Xue},
  Journal                  = {Superconductor Science and Technology},
  Year                     = {2016},
  Number                   = {12},
  Pages                    = {123001},
  Volume                   = {29},

  Abstract                 = {Cuprate high-temperature superconductors consist of two quasi-two-dimensional (2D) substructures: CuO 2 superconducting layers and charge reservoir layers. The superconductivity is realized by charge transfer from the charge reservoir layers into the superconducting layers without chemical dopants and defects being introduced into the latter, similar to modulation-doping in the semiconductor superlattices of AlGaAs/GaAs. Inspired by this scheme, we have been searching for high-temperature superconductivity in ultra-thin films of superconductors epitaxially grown on semiconductor/oxide substrates since 2008. We have observed interface-enhanced superconductivity in both conventional and unconventional superconducting films, including single atomic layer films of Pb and In on Si substrates and single unit cell (UC) films of FeSe on SrTiO 3 (STO) substrates. The discovery of high-temperature superconductivity with a superconducting gap of ∼20 meV in 1UC-FeSe/STO has stimulated tremendous interest in the superconductivity community, for it opens a new avenue for both raising superconducting transition temperature and understanding the pairing mechanism of unconventional high-temperature superconductivity. Here, we review mainly the experimental progress on interface-enhanced superconductivity in the three systems mentioned above with emphasis on 1UC-FeSe/STO, studied by scanning tunneling microscopy/spectroscopy, angle-resolved photoemission spectroscopy and transport experiments. We discuss the roles of interfaces and a possible pairing mechanism inferred from these studies.},
  File                     = {:Articles_FeSC/Reviews/FeSe_thin_films/Interface_high-temperature_superconductivity/WangL2016.pdf:PDF},
  Owner                    = {pierre},
  Timestamp                = {2016.10.17},
  Url                      = {http://stacks.iop.org/0953-2048/29/i=12/a=123001}
}

@Article{Wang2016g,
  Title                    = {Aspects of electron–phonon interactions with strong forward scattering in FeSe Thin Films on SrTiO 3 substrates},
  Author                   = {Y Wang and K Nakatsukasa and L Rademaker and T Berlijn and S Johnston},
  Journal                  = {Superconductor Science and Technology},
  Year                     = {2016},
  Number                   = {5},
  Pages                    = {054009},
  Volume                   = {29},

  Abstract                 = {Mono- and multilayer FeSe thin films grown on SrTiO 3 and BiTiO 3 substrates exhibit a greatly enhanced superconductivity over that found in bulk FeSe. A number of proposals have been advanced for the mechanism of this enhancement. One possibility is the introduction of a cross-interface electron–phonon (e–ph) interaction between the FeSe electrons and oxygen phonons in the substrates that is peaked in the forward scattering (small ##IMG## [http://ej.iop.org/images/0953-2048/29/5/054009/sustaa1beeieqn1.gif] {${\bf{q}})$} direction due to the two-dimensional nature of the interface system. Motivated by this, we explore the consequences of such an interaction on the superconducting state and electronic structure of a two-dimensional system using Migdal–Eliashberg (ME) theory. This interaction produces not only deviations from the expectations of conventional phonon-mediated pairing but also replica structures in the spectral function and density of states, as probed by angle-resolved photoemission spectroscopy, scanning tunneling microscopy/spectroscopy, and quasiparticle interference imaging. We also discuss the applicability of ME theory for a situation where the e–ph interaction is peaked at small momentum transfer and in the FeSe/STO system.},
  File                     = {:Articles_FeSC/FeSe/FeSe_thin_films/Wang2016.pdf:PDF},
  Keywords                 = {FeSe thin films, electron-phonon coupling},
  Owner                    = {pierre},
  Timestamp                = {2016.10.21},
  Url                      = {http://stacks.iop.org/0953-2048/29/i=5/a=054009}
}

@Article{Wang2016h,
  author    = {Haipeng Wang and Zirong Ye and Yan Zhang and Nanlin Wang},
  journal   = {Science Bulletin},
  title     = {Band structure reconstruction across nematic order in high quality FeSe single crystal as revealed by optical spectroscopy study},
  year      = {2016},
  issn      = {2095-9273},
  number    = {14},
  pages     = {1126 - 1131},
  volume    = {61},
  abstract  = {Abstract We perform an in-plane optical spectroscopy measurement on high quality FeSe single crystals grown by a vapor transport technique. Below the structural transition at Ts ∽90 K, the reflectivity spectrum clearly shows a gradual suppression around 400 cm−1 and the conductivity spectrum shows a peak at higher frequency. The energy scale of this gap-like feature is comparable to the width of the band splitting observed by ARPES. The low-frequency conductivity consists of two Drude components and the overall plasma frequency is smaller than that of the FeAs based compounds, suggesting a lower carrier density or stronger correlation effect. The plasma frequency becomes even smaller below Ts which agrees with the very small Fermi energy estimated by other experiments. Similar to iron pnictides, a clear temperature-induced spectral weight transfer is observed for FeSe, being indicative of strong correlation effect.},
  doi       = {http://dx.doi.org/10.1007/s11434-016-1102-2},
