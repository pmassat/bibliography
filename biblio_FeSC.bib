@Article{Fisher2011a,
  Title                    = {In-plane electronic anisotropy of underdoped '122' Fe-arsenide superconductors revealed by measurements of detwinned single crystals},
  Author                   = {I R Fisher and L Degiorgi and Z X Shen},
  Journal                  = {Reports on Progress in Physics},
  Year                     = {2011},
  Number                   = {12},
  Pages                    = {124506},
  Volume                   = {74},

  Abstract                 = {The parent phases of the Fe-arsenide superconductors harbor an antiferromagnetic ground state. Significantly, the Néel transition is either preceded or accompanied by a structural transition that breaks the four-fold symmetry of the high-temperature lattice. Borrowing language from the field of soft condensed matter physics, this broken discrete rotational symmetry is widely referred to as an Ising nematic phase transition. Understanding the origin of this effect is a key component of a complete theoretical description of the occurrence of superconductivity in this family of compounds, motivating both theoretical and experimental investigation of the nematic transition and the associated in-plane anisotropy. Here we review recent experimental progress in determining the intrinsic in-plane electronic anisotropy as revealed by resistivity, reflectivity and angle-resolved photoemission spectroscopy measurements of detwinned single crystals of underdoped Fe-arsenide superconductors in the '122' family of compounds.},
  File                     = {:Articles_FeSC/Reviews/Ba122_detwinned_electronic_anisotropy/FisherIR2011.pdf:PDF},
  Keywords                 = {Ba122, Ca122, Sr122, nematicity, detwinning, Resistivity, ARPES, Reflectivity, review},
  Owner                    = {pierre},
  Timestamp                = {2016.09.28},
  Url                      = {http://stacks.iop.org/0034-4885/74/i=12/a=124506}
}

